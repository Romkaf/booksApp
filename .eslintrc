{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"plugins": ["react"],
	"rules": {
		"no-console": "warn",
		"no-unused-vars": "warn",
		"react/jsx-uses-react": "error",
		"react/jsx-uses-vars": "error"
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	}
}
